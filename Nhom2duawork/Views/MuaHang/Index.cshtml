@{
    <!-- Views/MuaHang/Index.cshtml -->
    @model List<MuaHang>

    <h2>Danh sách Mua hàng</h2>

    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Tên Sản Phẩm</th>
                <th>Số Lượng</th>
                <th>Đơn Giá</th>
                <!-- Thêm các cột khác nếu cần -->
            </tr>
        </thead>
        <tbody>
            @foreach (var muaHang in Model)
            {
                <tr>
                    <td>@muaHang.Id</td>
                    <td>@muaHang.TenSanPham</td>
                    <td>@muaHang.SoLuong</td>
                    <td>@muaHang.DonGia</td>
                    <!-- Hiển thị các giá trị khác nếu cần -->
                </tr>
            }
        </tbody>
    </table>

}
